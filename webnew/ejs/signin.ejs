<div class="mask" id="mask" ></div>
<div class='opendiv' id="oDiv" data-bind="visible: isSignin">
    <script>
        document.getElementById("oDiv").style.marginTop="-185px";
        document.getElementById("oDiv").style.height="330px";
    </script>
    <div class="col-sm-3" style="margin-left: 270px">
        <button type="button" data-bind="click: gohome" class="newbtn" onmouseout="this.style.backgroundColor='white';" onmouseover="this.style.backgroundColor='grey';">X</button>
    </div>
    <form>
        <div class="log-title">
                <b>  Welcome  </b>
            </div>
            <div class="form-group" style="margin-left: 10px; margin-right: 10px;">
                <input data-bind="value: email" name="email" type="email" class="form-control" id="email2" placeholder="Your email address">
            </div>
            <div class="form-group" style="margin-left: 10px; margin-right: 10px;">
                <input data-bind="value: password" name="password" type="password" class="form-control" id="password2" placeholder="Create a password">
            </div>
            <div class="sign-invalid" data-bind="text:signinErrorMsg"></div>

        <div class="row">
            <div class="col-sm-3" style="margin-left: 30px">
                <button type="button" data-bind="click: submitSignin" class="btn btn-success">Sign in</button>
            </div>

            <div class="col-sm-7 sign-switch" style="margin-left: 20px">
                <p> No account? <a onclick="display2()">Sign up</a ></p >
            </div>
        </div>

    </form>
</div>