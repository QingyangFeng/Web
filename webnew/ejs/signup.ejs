<div class="mask" id="mask" ></div>
<div class='opendiv' id="oDiv1" data-bind="visible: isSignup" >
    <div class="col-sm-3" style="margin-left: 270px">
        <button type="button" data-bind="click: gohome" class="newbtn" onmouseout="this.style.backgroundColor='white';" onmouseover="this.style.backgroundColor='grey';">X</button>
    </div>
    <form>
        <div class="log-title">
            <b>Registration</b>
        </div>
        <div class="form-group" style="margin-left: 10px; margin-right: 10px;">
            <input data-bind="value: username, valueUpdate: 'afterkeydown'" name="username" type="text" class="form-control" id="username" placeholder="Pick a username">
        </div>
        <div class="form-group" style="margin-left: 10px; margin-right: 10px;">
            <input data-bind="value: email, valueUpdate: 'afterkeydown'" name="email" type="email" class="form-control" id="email" placeholder="Your email address">
        </div>
        <div class="form-group" style="margin-left: 10px; margin-right: 10px;">
            <input data-bind="value: password, valueUpdate: 'afterkeydown'" name="password" type="password" class="form-control" id="password" placeholder="Create a password">
        </div>
        <div class="sign-invalid" data-bind="text:signupErrorMsg"></div>
        <div class="row">
            <div class="col-sm-3"  style="margin-left: 20px">
                <button type="button" class="btn btn-success" data-bind="click: submitSignup">Sign up</button>
            </div>
            <div class="col-sm-7 sign-switch" style="margin-left: 30px">
                <p> Have an account? <a onclick="display1()">Sign in</a ></p >
            </div>
        </div>
    </form>
</div>